@{
}


<table class="job-listing">
    @foreach (var job in ViewBag.jobs)
    {
<tr>
    <td>
        <p>ID: @job.Id</p>
        <p>Name: @job.Name</p>
        <p>Employer: @job.Employer</p>
        <p>Location: @job.Location</p>
        <p>Position Type: @job.PositionType</p>
        <p>Skill: @job.CoreCompetency</p>
    </td>
</tr>}
</table>
